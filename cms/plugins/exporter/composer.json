{
    "name": "gustavomorais/craftexporter",
    "description": "Export and inport entries",
    "type": "craft-plugin",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "gustavomorais\\craftexporter\\": "src/"
        }
    },
    "extra": {
        "handle": "_exporter",
        "name": "Exporter",
        "developer": "Gustavo Morais",
        "documentationUrl": "",
        "class": "gustavomorais\\craftexporter\\Exporter"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

