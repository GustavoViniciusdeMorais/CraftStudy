{# {% dd products %} #}
<table>
    <thead>
        <th>#ID</th>
        <th>Name</th>
        <th>Price</th>
    </thead>
    <tbody>
    {% for item in products %}
        <tr>
            <td>{{item.id}}</td>
            <td>{{item.name}}</td>
            <td>{{item.price}}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
